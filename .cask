# Install native apps
brew tap phinze/homebrew-cask
brew install brew-cask

function installcask() {
	brew cask install "${@}" 2> /dev/null
}

installcask google-chrome
installcask tower
installcask adium
installcask alfred
installcask dropbox
installcask fantastical
installcask firefox
installcask f-lux
installcask kaleidoscope
installcask omni-focus
installcask sublime-text-3
installcask virtualbox
installcask codekit
installcask textexpander